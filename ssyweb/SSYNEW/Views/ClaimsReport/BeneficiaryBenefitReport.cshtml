            alert("Please Select SubDivision");
            $("#subdivid").focus();
            return false;
		}

		if ($("#schemeid").val() == "" || $("#schemeid").val() == null) {
			alert("Please Select Worker Type");
			$("#schemeid").focus();
			return false;
		}

		if ($("#benefitType").val() == "" || $("#benefitType").val() == null || $("#benefitType").val() == "") {
