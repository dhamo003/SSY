    //BenActionHistory

    function BenActionHistory(id) {





        // alert(id);

        $.get('@Url.Action("GetActionHistory", "Home")', { BenId: id }, function (result) {
            debugger;

            var table = $('.ActionHistory').DataTable();
            table.destroy();

            $('.ActionHistory tbody').empty();


            $.each(result, function (i, data) {

                var num = i + 1;
                debugger;
                //var uploaddata = data.BenPhoto + ',' + data.BenSignature + ',' + data.BenPassBook + ',' + data.BenIdentity + ',' + data.BenForm

                // alert(data.UploadDocument);
                $('.ActionHistory').append('<tr><td>' + num + '</td><td>' + data.User_Name + '</td><td>' + data.UserTypeName + '</td><td>' + data.Type + '</td><td>' + data.Reason + '</td><td>' + data.CreatedDate + '</td></tr>')

            });


            $('.ActionHistory').DataTable({

            });
        });
    }

    //Print
    function printDiv() {

        var printContents = document.getElementById('prntform').innerHTML;

        $("fieldset.custom-border").css('border', 'none !important');

        //    alert(printContents);
        w = window.open();
        w.document.write(printContents);
        w.print();
        w.close();
    }

	function Bentrnasclick(id) {

		$(".regpop").modal("show");

		$('#revived').hide();

		var checkAppStatus = $("#BeniAppSt").val();

		if (checkAppStatus != 0) {

			$("#divfooter").hide();

		}


		if ($("#AcUserType").val() > 1 && $("#pageval").val() == 1) {


			var Schemeid = $("#schemeid").val();

			$.get('@Url.Action("BenSaveActionHistory", "Home")', { UserId: $("#AcDepUserId").val(), BenUserId: id, Reason: "Viewed", Type: "Viewed", Schemeid: Schemeid }, function (res) {

				// alert(res);
			});
		}


		$.get('@Url.Action("GetbenDetails", "Registration")', { bensno: id }, function (res) {

			var IsUnique = res.IsUnique;

			if (IsUnique == "P") {
				$("#divfooter").hide();
			}

			if (IsUnique == "Y") {
				//$("#divfooter").show();
				$("#UVerified").show();
				$("#DVerified").hide();
			}

			if (IsUnique == "N") {
				$("#DVerified").show();
				$("#UVerified").hide();
			}



			if (res.Status == 3) {
				$("#divSentbackRem").show()

				$("#lblSentbackRem").text(res.Remarks).addClass('label-data').css({ color: "red!important" });

				$("#divpendingremark").hide()
			}

			if (res.Status == 2) {
				debugger;
				//  alert(res.Remarks);
				$("#divpendingremark").show()

				$("#divSentbackRem").hide()

				$("#lblremarkPend").text(res.Remarks).addClass('label-data').css({ color: "red!important" });

				//alert(res.RejectedDocumnet);
				if (res.RejectedDocumnet != "") {
					//$("#ImgRejectedDoc").show();
					$("#ImgRejectedDoc").attr('src', res.RejectedDocumnet);

				}
				else {
					$("#ImgRejectedDoc").hide();
				}
				//  alert(res.RegType);
				if (res.RegType == "N") {

					$('#revived').hide();
				}
				else {
					$('#revived').show();
				}
			}

			if (res.Status == "0" || res.Status == "1") {
				$("#divpendingremark").hide()
