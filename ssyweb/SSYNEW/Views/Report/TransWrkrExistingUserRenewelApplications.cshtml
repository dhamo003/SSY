    function SubmitStatus(id)
    {

        var refid = $("#lblrefid").text();
        var Remarks =  $("#RejectedRemarks").val();
        if(id == 2)
        {
            if(Remarks == "" || Remarks == null)
            {
                alert("Please Enter Remarks.");
                $("#RejectedRemarks").focus();
                return false;
            }
        }
        if(id == 1)
            Remarks = "";
         $.get('@Url.Action("VerifyRenewalAppliationsfromSLO", "Report")', { id: refid, Remarks: Remarks, Status: id}, function (res) {
            alert(res.Message);
			 window.location.href = " /Report/TransWrkrExistingUserRenewelApplications";

