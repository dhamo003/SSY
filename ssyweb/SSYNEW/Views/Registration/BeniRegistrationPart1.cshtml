    function printDiv() {


        $("#divDocument").show();
        $("#divDocumentBengali").show();

        var divToPrint = "";

        var langid = $("#ddlLanguage").val();
        if(langid == "1")
        {
            divToPrint = document.getElementById('DivPrint');

        }
        if(langid == "2")
        {
            divToPrint = document.getElementById('DivPrintBengali');

        }

        var divDocument = document.getElementById('divDocument');
        var newWin = window.open('', 'Print-Window');
        newWin.document.open();
        newWin.document.write('<html><body onload="window.print()">' + divToPrint.innerHTML + '</body></html>');

        newWin.document.close();
        setTimeout(function() {
            newWin.close();
        }, 10);

        $("#divDocument").hide();
        $("#divDocumentBengali").hide();
    }

