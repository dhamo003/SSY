@model SSY.Models.SignUpUser
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_MainHomeLayout.cshtml";
}
<script src="~/Scripts/jquery.min.js"></script>
<div class="welcome-bg-inside">
    <div class="row">
        <div class="col-sm-12">
            <div class=" content-box-inner">
                <h1 class="inner-page-title nopadbot text-left">Contact Us</h1>
               <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3721040.384605427!2d88.39653321815399!3d24.387268747463285!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x39fa16ff1397e887%3A0x71543a3dc3e7a20a!2sWest+Bengal!5e0!3m2!1sen!2sin!4v1496492669788" width="100%" height="400" frameborder="0" style="border:0" allowfullscreen></iframe>
                <!---------------------------------------------------------------------------------------------------------------->
                <br class="clearfix">
                <div id="datatable-1_wrapper" class="dataTables_wrapper" role="grid">
                   
                 
                    <table id="example" class="table table-striped table-bordered contactdet dt-responsive nowrap" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th><Label id="lblaname">Name</Label></th>
                                <th><label>Designation</label></th>
                                <th><label>Phone</label></th>
                                <th><label>Officer Name</label></th>
                                <th><label>Email</label></th>
                            </tr>
                        </thead>
                    </table>
             
                    
                </div>
                <a href="~/Home/ContactMore"><i class="fa fa-phone" aria-hidden="true"></i><strong>More Information</strong></a>
                <style>
                    a {
                        cursor: pointer;
                    }
                </style>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        LoadDeptdata();
       
    });
   
    function LoadDeptdata()
    {
         
     
        $.get('@Url.Action("GetContactDataDet", "Home")', {  }, function (data) {
            //datasource = data;
             
            var res = JSON.stringify(data);
            $('#example').DataTable({
                "aaData": JSON.parse(res),
                "columns": [{ "data": "DRLName" }, { "data": "UserTypeName" }, { "data": "Mobileno" }, { "data":  "Name" }, { "data": "Email" }]
            });
        });
       
    }
</script>
